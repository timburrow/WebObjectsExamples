Anchor: WOGenericContainer {
	elementName = "a";
	name = anchor;
}

ArrowActiveImage: WOActiveImage {
	action = toggleVisibilityClicked;
	align = "TOP";
	alt = helpString;
	border = "0";
	disabled = ^disabled;
	filename = currentArrowImageName;
	fragmentIdentifier = anchor;
	framework = framework;
}

ArrowImage: WOImage {
	align = "MIDDLE";
	alt = helpString;
	border = "0";
	filename = currentArrowImageName;
	framework = framework;
	name = helpString;
}

ArrowLink: WOHyperlink {
	action = toggleVisibilityClicked;
	disabled = ^disabled;
	fragmentIdentifier = anchor;
}

Conditional: WOConditional {
	condition = isVisible;
}

Content: WOComponentContent {
}

IsInFormConditional: WOConditional {
	condition = hasSubmitAction;
}

IsNotInFormConditional: WOConditional {
	condition = hasSubmitAction;
	negate = true;
}

LabelString: WOString {
	escapeHTML = false;
	value = label;
}

LabelString2: WOString {
	escapeHTML = false;
	value = label;
}

NotConditional: WOConditional {
	condition = isVisible;
	negate = true;
}
