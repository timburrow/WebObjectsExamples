<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="WOTable" wocomponentcontent="true">
		<image path="WOTable.tiff"/>

		<binding name="col"/>
		<binding name="index"/>
		<binding name="list"/>
		<binding name="maxColumns"/>
		<binding name="row"/>
		<binding name="item"/>
		<binding name="tableBackgroundColor"/>
		<binding name="border"/>
		<binding name="cellpadding"/>
		<binding name="cellspacing"/>
		<binding name="rowBackgroundColor"/>
		<binding name="cellBackgroundColor"/>
		<binding name="cellAlign"/>
		<binding name="cellVAlign"/>
		<binding name="cellWidth"/>
		<binding name="tableWidth"/>

		<validation message="&apos;list&apos; is a required binding">
			<unbound name="list"/>
		</validation>

		<validation message="&apos;item&apos; is a required binding">
			<unbound name="item"/>
		</validation>

		<validation message="&apos;item&apos; must be bound to a settable value">
			<unsettable name="item"/>
		</validation>
	</wo>
</wodefinitions>